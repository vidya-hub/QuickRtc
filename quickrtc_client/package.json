{
  "name": "quickrtc-client",
  "version": "2.1.0",
  "description": "Simple, event-driven WebRTC conferencing library built on mediasoup",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:watch": "webpack --mode=development --watch",
    "build:tsc": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "webrtc",
    "mediasoup",
    "client",
    "browser",
    "real-time",
    "video",
    "audio",
    "conferencing"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.18.1",
  "dependencies": {
    "mediasoup-client": "^3.16.7",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
