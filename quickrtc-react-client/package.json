{
  "name": "quickrtc-react-client",
  "version": "2.1.0",
  "description": "React hooks and components for QuickRTC WebRTC conferencing",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && webpack --mode=production",
    "build:dev": "tsc && webpack --mode=development",
    "build:watch": "tsc --watch & webpack --mode=development --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "webrtc",
    "mediasoup",
    "react",
    "hooks",
    "client",
    "real-time",
    "video",
    "audio",
    "conference"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "quickrtc-client": "^2.0.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "@types/react": "^18.3.18",
    "react": "^18.3.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
