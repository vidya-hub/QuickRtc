{
  "name": "quickrtc-react-client",
  "version": "1.1.0",
  "description": "Production-ready React client library for QuickRTC with Redux state management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && webpack --mode=production",
    "build:dev": "tsc && webpack --mode=development",
    "build:watch": "tsc --watch & webpack --mode=development --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "webrtc",
    "mediasoup",
    "react",
    "redux",
    "client",
    "real-time",
    "video",
    "audio",
    "conference"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-redux": "^8.0.0 || ^9.0.0",
    "@reduxjs/toolkit": "^2.0.0"
  },
  "dependencies": {
    "mediasoup-client": "^3.16.7",
    "quickrtc-types": "^1.0.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "@types/node": "^24.7.1",
    "@types/react": "^18.3.18",
    "react": "^18.3.1",
    "react-redux": "^9.2.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
